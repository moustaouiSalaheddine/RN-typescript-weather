{
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".js",
                    ".ios.js",
                    ".android.js",
                    ".jsx",
                    ".ts",
                    ".tsx"
                ],
                "alias": {
                    "@assets": "./src/assets",
                    "@icons": "./src/assets/icons",
                    "@navigations": "./src/navigations",
                    "@screens": "./src/screens",
                    "@core": "./src/core",
                    "@constants": "./src/core/utils/constants",
                }
            }
        ]
    ]
}